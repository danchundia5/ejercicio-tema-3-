public class Main {
    public static void main(String[] args){
        coche micoche = new coche();
        suma(2,2 );
    }
    public static int suma(int a, int b) {
        int resultado = a + b;
        System.out.println(resultado);

        return suma(a, resultado);
    }

}

   class coche {
    public int puertas = 4

    public void setPuertas() {
           this.puertas--;
       }
   }
